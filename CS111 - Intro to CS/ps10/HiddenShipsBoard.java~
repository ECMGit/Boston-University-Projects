public class HiddenShipsBoard extends Board{
  
  
  public HiddenShipsBoard(int dimension) {
    super(dimension);
  }
  
  public char getStatusChar(int row, int col) {
    if (previousHit(row, col)) {
      return 'X';
    } else if (previousMiss(row, col)) {
      return '-';
    } else {
      return ' ';
    }
  }   
}